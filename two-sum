class Solution {
    public int[] twoSum(int[] nums, int target) {
        int[] arr = new int[2];


        for(int i = 0; i < nums.length; i++){
            int j = target - nums[i];
            for(int k = 0; k < nums.length; k++){
                if (nums[k] == j) {
                    arr[0] = k;
                    arr[1] = i;
                    break;
                }
            }

        }
        return arr;

    }
}
